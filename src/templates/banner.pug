section.banner(data-animate='clip-path')
  .banner-bg

  ._container.banner__container
    .banner__wrapper
      .banner__main
        .banner__text
          h1.banner__title
            span Професійне
            span створення
            span.labeled
              span.labeled__label
                <svg width="40" height="40" viewBox="0 0 40 40" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path d="M28.334 28.3333L36.6673 20L28.334 11.6667M11.6673 11.6667L3.33398 20L11.6673 28.3333M23.334 5L16.6673 35" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
              span сайтів

          p.banner__subtitle= 'Створимо кращий сайт у вашій ніші, забезпечимо нескінченний потік клієнтів'

        .banner__actions
          button.banner__button.button-dark.popup-open(
            data-popup='_popup-form'
          )
            span Обговорити проект

            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M7 17L17 7M17 7H7M17 7V17" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>

          .banner__glob.glob.mobile
            img(src=require('@/assets/img/banner/glob.png'), alt='glob')

          button.banner-circle-button
            span.banner-circle-button__container
              span Останні роботи

              i.banner-circle-button__outer
                span.banner-circle-button__text
                  img(
                    src=require('@/assets/img/banner/circle-button-text.svg'),
                    alt='text'
                  )

                span.banner-circle-button__ico
                  <svg width="36" height="36" viewBox="0 0 36 36" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M9 27L27 9M27 9H15M27 9V21" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                  </svg>

        .banner__glob.glob.desktop
          img(src=require('@/assets/img/banner/glob.png'), alt='glob')

      .banner__footer
        ul.socials.banner__socials
          each item in socialsBanner
            li
              a.socials__link(
                href=`${item.href}`,
                target='_blank',
                rel='noreferrer'
              )!= item.label

        a.banner-download(href='#') 
          span.banner-download__label
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
